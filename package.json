{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve --type=dev",
    "dev:t": "vue-cli-service serve --type=test",
    "dev:pre": "vue-cli-service serve --type=pre",
    "dev:prod": "vue-cli-service serve --type=prod",
    "dev:prod2": "vue-cli-service serve --type=prod2",
    "build": "vue-cli-service build",
    "build:test": "vue-cli-service build --type=test",
    "build:pre": "vue-cli-service build --type=pre",
    "build:prod": "vue-cli-service build --type=prod",
    "upload": "vue-cli-service build --type=test && gulp start --type=test",
    "upload:pre": "vue-cli-service build --type=pre && gulp start --type=pre",
    "upload:prod": "vue-cli-service build --type=prod && gulp start --type=prod",
    "upload:prod2": "vue-cli-service build --type=prod2 && gulp start --type=prod2",
    "upload:lan": "vue-cli-service build --type=lan && gulp start --type=lan"
  },
